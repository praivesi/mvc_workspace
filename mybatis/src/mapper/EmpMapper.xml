<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
 <mapper namespace=" ">
 	<select id="selectAllEmp" resultType="vo.EmpVO">
 		select * from emp
 	</select>
 	
 	<select id="selectOneByEName" parameterType="String">
 		select * from emp
 		where ename = #{ename}
 	</select>
 </mapper>